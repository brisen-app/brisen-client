{
  "name": "brisen-client",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --go",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "testCI": "jest --ci --coverage"
  },
  "jest": {
    "preset": "jest-expo",
