    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.18.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "expo": "~50.0.11",
    "expo-blur": "~12.9.2",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-dev-client": "~3.3.11",
    "expo-font": "~11.10.2",
    "expo-image": "~1.10.6",
    "expo-linear-gradient": "~12.7.2",
