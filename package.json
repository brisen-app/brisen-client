{
  "name": "brisen-client",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo prebuild -p all && expo --dev-client",
    "android": "expo run:android --device",
    "android-release": "expo run:android --variant release --device",
    "ios": "expo run:ios --device",
    "ios-release": "expo run:ios --configuration Release --device",
    "web": "expo start --web",
    "upgrade": "npm update && npm install expo@latest && npx expo install --fix",
    "generate-types": "npx supabase gen types typescript --project-id tlnldlywflpgvjepxwxz --schema public > models/supabase.ts",
    "build": "eas build -p all --profile develop --non-interactive",
    "test": "jest --watchAll",
    "test-ci": "jest --ci --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "setup.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)",
      "/coverage/",
      "/app/",
      "/components/",
      "/\\.",
      "\\.(config|d)\\.(js|jsx|ts|tsx|json)"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "jsxSingleQuote": true,
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "expo": {
    "autolinking": {
      "ios": {
        "exclude": [
          "expo-navigation-bar"
        ]
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.6.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^7.0.0",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-query": "^5.18.1",
    "expo": "^52.0.9",
    "expo-application": "~6.0.1",
    "expo-build-properties": "~0.13.1",
    "expo-clipboard": "~7.0.0",
    "expo-constants": "~17.0.3",
    "expo-crypto": "~14.0.1",
    "expo-dev-client": "~5.0.3",
    "expo-font": "~13.0.1",
    "expo-image": "~2.0.1",
    "expo-insights": "~0.8.1",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~7.0.3",
    "expo-localization": "~16.0.0",
    "expo-navigation-bar": "~4.0.3",
    "expo-router": "~4.0.7",
    "expo-splash-screen": "~0.29.12",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.3",
    "expo-updates": "~0.26.8",
    "expo-web-browser": "~14.0.1",
    "jest": "^29.3.1",
    "jest-expo": "~50.0.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.2",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-purchases": "^8.1.0",
    "react-native-purchases-ui": "^8.1.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.0.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/react-native": "^12.6.1",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "react-test-renderer": "18.2.0",
    "supabase": "^1.136.3",
    "typescript": "~5.3.3"
  },
  "private": true
}
