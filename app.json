{
    "expo": {
        "name": "Brisen",
        "slug": "brisen-client",
        "version": "0.0.1",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "no.kallerud",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "no.kallerud.brisen"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "no.kallerud.brisen"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router", "expo-localization"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "ea66c9bc-ab42-4370-b12b-b6db5bf784c8"
            }
        },
        "owner": "brisen"
    }
}
