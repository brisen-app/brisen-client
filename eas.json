{
    "build": {
        "develop": {
            "extends": "preview",
            "developmentClient": true,
            "env": {
                "ENVIRONMENT": "develop"
            }
        },
        "preview": {
            "extends": "production",
            "distribution": "internal",
            "env": {
                "ENVIRONMENT": "preview"
            }
        },
        "production": {
            "env": {
                "ENVIRONMENT": "production"
            },
            "android": {
                "image": "latest"
            },
            "ios": {
                "image": "latest"
            }
        }
    }
}
